@model List<WebApplication.Models.Acessary_Log_Key>
@{
    var index = 0;
}
<style>
    .money {
        text-align: right;
    }

    label {
        padding-right: 5px;
    }
</style>
<div class="card">
    <div class="card-header">
        <h3 class="card-title">Lịch sử xuất linh kiện</h3>
    </div>
    <div class="card-body">
        <table class="table table-bordered">
            <tr style="background-color:grey">
                <th>#</th>
                <th>Mã phiếu</th>
                <th>Linh kiện</th>
                <th>Số lượng</th>
                <td>Ngày tạo</td>
            </tr>
            @foreach (var item in Model)
            {
                ++index;
                <tr>
                    <td>@index</td>
                    <td>@item.Code</td>
                    <td>@item.Accessary</td>

                    <td>@item.Count</td>
                    <td>@item.Createdate.Value.ToString("dd/MM/yyyy")</td>

                </tr>
            }
        </table>
    </div>
    
</div>
<div class="text-center">
    <button type="button" class="btn btn-danger" data-dismiss="modal"><i class="ti ti-close"></i> Thoát</button>
</div>


