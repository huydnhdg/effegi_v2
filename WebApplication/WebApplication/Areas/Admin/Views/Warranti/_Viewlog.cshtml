@model List<WebApplication.Models.Warranti_Log>
@{
    string code = ViewBag.Code;
}

<div class="card">
    <div class="card-header">
        <h3 class="card-title">Thông tin bảo hành <span class="text-danger">@code</span></h3>
    </div>
    <div class="card-body">
        @foreach (var item in Model)
        {
            <p>@item.Createdate.Value.ToString("dd/MM/yyyy HH:mm") @item.Description</p>
        }
        <div class="form-group text-center">
            <button type="button" class="btn btn-danger" data-dismiss="modal"><i class="ti ti-close"></i> Thoát</button>
        </div>
    </div>
</div>
